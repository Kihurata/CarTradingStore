{
  "name": "backend",
  "version": "1.0.0",
  "description": "Backend for CarTradingStore",
  "main": "dist/server.js",
  "type": "commonjs",
  "scripts": {
  "dev": "ts-node --transpile-only src/server.ts",
  "build": "tsc",
  "start": "node dist/server.js",
  "lint": "eslint src/**/*.ts",
  "test": "cross-env NODE_ENV=test jest",
  "test:watch": "cross-env NODE_ENV=test jest --watch",
  "test:unit": "cross-env NODE_ENV=test jest tests/unit",
  "test:int": "cross-env NODE_ENV=test jest tests/integration --runInBand --forceExit",
  "test:all": "cross-env NODE_ENV=test jest --runInBand --forceExit",
  "test:coverage": "cross-env NODE_ENV=test jest --coverage",
  "test:coverage:all": "cross-env NODE_ENV=test jest --coverage --runInBand --forceExit"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.46.1",
    "@types/cookie-parser": "^1.4.9",
    "@types/cors": "^2.8.19",
    "axios": "^1.7.4",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "node-fetch": "^3.3.2",
    "nodemailer": "^6.9.13",
    "pdfkit": "^0.15.0",
    "pg": "^8.11.3",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/morgan": "^1.9.4",
    "@types/multer": "^1.4.7",
    "@types/node": "^20.10.5",
    "@types/nodemailer": "^7.0.2",
    "@types/pdfkit": "^0.17.3",
    "@types/pg": "^8.10.9",
    "@types/uuid": "^8.3.4",
    "eslint": "^8.56.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.2",
    "@types/jest": "^29.5.12",
    "@types/supertest": "^6.0.2",
    "cross-env": "^7.0.3",
    "jest": "^29.7.0",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5"
  }
}
